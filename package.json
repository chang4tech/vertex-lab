{
  "name": "vertex-lab-app",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "release": "node tools/release.mjs",
    "release:patch": "node tools/release.mjs patch",
    "release:minor": "node tools/release.mjs minor",
    "release:major": "node tools/release.mjs major",
    "milestones": "node tools/milestones.mjs",
    "pretest:e2e": "npm run build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:mobile": "playwright test --project='Mobile Safari (WebKit)'",
    "test:e2e:ff": "playwright test --project='Mobile Firefox'",
    "playwright:install:all": "npx playwright install chromium firefox webkit",
    "playwright:install:chromium": "npx playwright install chromium",
    "playwright:install:webkit": "npx playwright install webkit",
    "playwright:install:mirror": "PLAYWRIGHT_DOWNLOAD_HOST=https://playwright.azureedge.net npx playwright install chromium firefox webkit"
  },
  "dependencies": {
    "playwright": "^1.55.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^22.1.0",
    "vite": "^7.1.2",
    "vitest": "^0.34.6"
  }
}
